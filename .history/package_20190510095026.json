{
  "name": "umi-electron",
  "version": "1.0.0",
  "description": "an example based on umijs + electron",
  "main": "./dist/main/main.js",
  "private": true,
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "appId": "umi-electron",
    "asar": false
  },
  "scripts": {
    "start": "npm run build-main-dev && electron ./dist/main/main.js",
    "start:main": "electron-webpack dev",
    "start:renderer": "APP_ROOT=src/renderer umi dev",
    "build-main-prod": "NODE_ENV=production webpack --config ./build/webpack.main.prod.config.js",
    "build-main-dev": "NODE_ENV=development webpack --config ./build/webpack.main.config.js",
    "build:renderer": "APP_ROOT=src/renderer umi build",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "debug:main": "electron --inspect=5858 -w ./dist/main/main.js",
    "pack": "npm run build:renderer && npm run build-main-prod && electron-builder --dir",
    "dist": "electron-builder",
    "prettier": "prettier --list-different \"./**/*.{ts,tsx,js,jsx,less}\""
  },
  "keywords": [
    "Electron",
    "umi",
    "quick",
    "start",
    "tutorial",
    "demo",
  ],
  "author": {
    "name": "xiaobei",
    "email": "yebeixiaohai@gmail.com"
  },
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.2.0",
    "electron-webpack": "^2.6.2",
    "prettier": "1.17.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "umi": "^2.6.17",
    "umi-plugin-react": "^1.7.6",
    "webpack": "4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "http-proxy": "^1.17.0"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": null
    }
  }
}